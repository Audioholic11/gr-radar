<block>
  <name>Soapysdr echotimer</name>
  <key>radar_soapysdr_echotimer</key>
  <category>[RADAR]</category>
  <import>import radar</import>
  <make>radar.soapysdr_echotimer($samp_rate, $center_freq, $num_delay_samps, $args, $antenna_tx, $gain_tx, $timeout_tx, $wait_tx, $lo_offset_tx, $antenna_rx, $gain_rx, $timeout_rx, $wait_rx, $lo_offset_rx, $len_key)</make>
  <param>
    <name>Samp_rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Center_freq</name>
    <key>center_freq</key>
    <type>float</type>
  </param>
  <param>
    <name>Num_delay_samps</name>
    <key>num_delay_samps</key>
    <type>int</type>
  </param>
  <param>
    <name>Args</name>
    <key>args</key>
    <value>"device args"</value>
    <type>string</type>
  </param>
  <param>
    <name>Antenna_tx</name>
    <key>antenna_tx</key>
    <value>"BAND2"</value>
    <type>string</type>
  </param>
  <param>
    <name>Gain_tx</name>
    <key>gain_tx</key>
    <value>0</value>
    <type>float</type>
  </param>
  <param>
    <name>Timeout_tx</name>
    <key>timeout_tx</key>
    <value>.1</value>
    <type>float</type>
  </param>
  <param>
    <name>Wait_tx</name>
    <key>wait_tx</key>
    <value>.001</value>
    <type>float</type>
  </param>
  <param>
    <name>Lo_offset_tx</name>
    <key>lo_offset_tx</key>
    <value>0</value>
    <type>float</type>
  </param>
  <param>
    <name>Antenna_rx</name>
    <key>antenna_rx</key>
    <value>"LNAH"</value>
    <type>string</type>
  </param>
  <param>
    <name>Gain_rx</name>
    <key>gain_rx</key>
    <value>0</value>
    <type>float</type>
  </param>
  <param>
    <name>Timeout_rx</name>
    <key>timeout_rx</key>
    <value>.1</value>
    <type>float</type>
  </param>
  <param>
    <name>Wait_rx</name>
    <key>wait_rx</key>
    <value>.001</value>
    <type>float</type>
  </param>
  <param>
    <name>Lo_offset_rx</name>
    <key>lo_offset_rx</key>
    <value>0</value>
    <type>float</type>
  </param>
  <param>
    <name>Len_key</name>
    <key>len_key</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
